{
    "expo": {
        "name": "EasyCovid",
        "slug": "EasyCovid",
        "description": "Application Android & iOS permettant la génération d'attestation de sortie durant le confinement de la pandémie mondiale COVID-19.",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "momotoculteur.easycovid",
            "buildNumber": "1.0.0"
        },
        "android": {
            "package": "com.momotoculteur.easycovid",
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#FFFFFF"
            },
            "versionCode": 1
        },
        "web": {
            "favicon": ""
        }
    }
}