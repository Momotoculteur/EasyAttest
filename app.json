{
    "expo": {
        "name": "EasyAttest",
        "slug": "EasyAttest",
        "description": "Application Android & iOS permettant la génération d'attestation de sortie durant le confinement de la pandémie mondiale COVID-19.",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./src/assets/icon.png",
        "splash": {
            "image": "./src/assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.momotoculteur.easyattest",
            "buildNumber": "1",
            "config": {
                "usesNonExemptEncryption": false
            }
        },
        "android": {
            "package": "com.momotoculteur.easyattest",
            "permissions": [],
            "adaptiveIcon": {
                "foregroundImage": "./src/assets/adaptive-icon.png",
                "backgroundColor": "#FFFFFF"
            },
            "versionCode": 1
        },
        "web": {
            "favicon": ""
        }
    }
}
